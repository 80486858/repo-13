<html>
<body>
<h1>FooResource!</h1>

<p>Hello from a FooResource!</p>

<% val it = resource[com.mh.serverpages.resources.FooResource] %>

<p>FooResource is ${it}</p>

<p>
  Attributes
  <ul>
  <%
      val e = request.getAttributeNames
      while (e.hasMoreElements) {
  %>
    <li>${e.nextElement}</li>
  <% } %>
  </ul>
</p>

</body>
</html>